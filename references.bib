}

@article{gao_hull_2016,
	title = {Hull shape optimization for autonomous underwater vehicles using {CFD}},
	volume = {10},
	issn = {1994-2060},
	url = {https://doi.org/10.1080/19942060.2016.1224735},
	doi = {10.1080/19942060.2016.1224735},
	abstract = {Drag estimation and shape optimization of autonomous underwater vehicle ({AUV}) hulls are critical to energy utilization and endurance improvement. In the present work, a shape optimization platform composed of several commercial software packages is presented. Computational accuracy, efficiency and robustness were carefully considered and balanced. Comparisons between experiments and computational fluid dynamics ({CFD}) were conducted to prove that a two-dimensional (2D) unstructured mesh, a standard wall function and adaptive mesh refinement could greatly improve efficiency as well as guarantee accuracy. Details of the optimization platform were then introduced. A comparison of optimizers indicates that the multi-island genetic algorithm ({MIGA}) obtains a better hull shape than particle swarm optimization ({PSO}), despite being a little more time consuming. The optimized hull shape under general volume requirement could provide reference for {AUV} hull design. Specific requirements based on optimization testify of the platform’s robustness.},
	pages = {599--607},
	number = {1},
	journaltitle = {Engineering Applications of Computational Fluid Mechanics},
	author = {Gao, Ting and Wang, Yaxing and Pang, Yongjie and Cao, Jian},
	urldate = {2024-11-19},
	date = {2016-01-01},
	note = {Publisher: Taylor \& Francis
\_eprint: https://doi.org/10.1080/19942060.2016.1224735},
	file = {Full Text PDF:C\:\\Users\\0\\Zotero\\storage\\ZA4VUWML\\Gao et al. - 2016 - Hull shape optimization for autonomous underwater .pdf:application/pdf},
}

@article{lin_hydrodynamic_2022,
	title = {Hydrodynamic optimization in high-acceleration underwater motions using added-mass coefficient},
	volume = {263},
	issn = {0029-8018},
	url = {https://www.sciencedirect.com/science/article/pii/S0029801822015803},
	doi = {10.1016/j.oceaneng.2022.112274},
	abstract = {Hydrodynamics plays a significant role in the underwater motions, environmental disturbances, and kinematic characteristics of autonomous underwater vehicles ({AUVs}). However, the added mass coefficient has typically not been considered in analyzing the underwater motions of {AUVs}. Here is report the combustion actuated autonomous underwater vehicles ({CA}-{AUVs}). The high-acceleration motions (reaching ∼103 ms−2 in 8.9 ms) of the {CA}-{AUVs} are analyzed by simultaneously considering the added mass and drag coefficients. The drag coefficient is analyzed by investigating a steady computational fluid dynamic ({CFD}) model whilst analyzing the added mass coefficient by developing an unsteady {CFD} model. Relations between the coefficients and parameterized geometric dimensions are obtained and applied to predict the underwater motions, which quantitatively explain the hydrodynamic principle of {CA}-{AUVs} underwater motions. Hydrodynamic optimizations are conducted with the added mass and drag coefficients to further investigate the influences of the structural design on the {CA}-{AUVs} kinematic performances. The research findings provide useful information on the hydrodynamic mechanisms and optimizations of the high-acceleration underwater motions.},
	pages = {112274},
	journaltitle = {Ocean Engineering},
	shortjournal = {Ocean Engineering},
	author = {Lin, Guanzheng and Yang, Yang and He, Zhiguo and Jiao, Pengcheng},
	urldate = {2024-11-05},
	date = {2022-11-01},
	keywords = {Combustion actuation, Combustion-actuated autonomous underwater vehicle ({CA}-{AUVs}), Hydrodynamics, Optimization},
	file = {Lin et al. - 2022 - Hydrodynamic optimization in high-acceleration und.pdf:C\:\\Users\\0\\Zotero\\storage\\5DGI58T6\\Lin et al. - 2022 - Hydrodynamic optimization in high-acceleration und.pdf:application/pdf;ScienceDirect Snapshot:C\:\\Users\\0\\Zotero\\storage\\JXBBMK3N\\S0029801822015803.html:text/html},
}

@article{liu_fine_2021,
	title = {A fine drag coefficient model for hull shape of underwater vehicles},
	volume = {236},
	issn = {0029-8018},
	url = {https://www.sciencedirect.com/science/article/pii/S0029801821007770},
	doi = {10.1016/j.oceaneng.2021.109361},
	abstract = {The increasingly diverse mission requirements result in challenges to the rapid design and functional diversification of underwater vehicles ({UVs}). The genealogical design of {UVs} shape is one of the key steps to deal with these challenges. In the present work, based on the hydrodynamic characteristics with different shape features of {UVs}, the hull shape genealogy including the spherical/spheroidal shape, slender axisymmetric shape, slender flat shape and round-dish shape was established according to the genealogy classification theory. Influence of the hull shape parameters and Reynolds numbers on hydrodynamics of the spherical/spheroidal shape, slender axisymmetric shape and slender flat shape for wide application in hull shape genealogy was investigated by using computational fluid dynamics ({CFD}) simulation method. The mathematical models of the drag coefficients of the hull shape genealogy with different shape parameters and Reynolds numbers were established by fitting the data obtained from {CFD} calculation. The accuracy of the established drag coefficient models was verified by the tank tests of “Petrel-L″ model developed by Tianjin University and {SUBOFF} model. Furthermore, the established drag coefficient models show higher accuracy than the traditional drag coefficient models, namely G\&J model, {VT} model and {MIT} model, by comparing with the towing tank tests of {SUBOFF} model. The present research provides an important reference for realizing the rapid and efficient design of {UVs} with the same hydrodynamic shapes.},
	pages = {109361},
	journaltitle = {Ocean Engineering},
	shortjournal = {Ocean Engineering},
	author = {Liu, Yuhong and Yu, Zhenji and Zhang, Lianhong and Liu, Tiantian and Feng, Dengxue and Zhang, Jinkai},
	urldate = {2024-11-04},
	date = {2021-09-15},
	keywords = {Underwater vehicles, Drag coefficient map, Drag coefficient model, Hull shape genealogy, Rapid design},
	file = {Liu et al. - 2021 - A fine drag coefficient model for hull shape of un.pdf:C\:\\Users\\0\\Zotero\\storage\\9BM55YJ8\\Liu et al. - 2021 - A fine drag coefficient model for hull shape of un.pdf:application/pdf;ScienceDirect Snapshot:C\:\\Users\\0\\Zotero\\storage\\U2E432EL\\S0029801821007770.html:text/html},
}

@article{alam_design_2017,
