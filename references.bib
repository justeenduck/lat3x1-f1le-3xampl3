	urldate = {2024-07-02},
	date = {2017-09},
	keywords = {{ROV}, Aquaculture, Buoyancy, Cameras, Concept design, marine aquaculture applications, Monitoring, motion control, Robots, Temperature sensors},
	file = {IEEE Xplore Abstract Record:C\:\\Users\\0\\Zotero\\storage\\E9S922G6\\8232180.html:text/html;IEEE Xplore Full Text PDF:C\:\\Users\\0\\Zotero\\storage\\DGRGJQLF\\Osen et al. - 2017 - A novel low cost ROV for aquaculture application.pdf:application/pdf},
